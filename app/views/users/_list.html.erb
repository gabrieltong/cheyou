<%- model_class = User -%>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:username) %></th>
      <th><%= model_class.human_attribute_name(:froms) %></th>      
      <th><%= model_class.human_attribute_name(:tos) %></th>            
      <th><%= model_class.human_attribute_name(:cars) %></th>                  
    </tr>
  </thead>
  <tbody>
    <% collection.each do |user| %>
      <tr>
        <td><%= link_to user.email, user_path(user) %></td>
        <td><%= render 'tags/input',:taggable=>user,:owner=>current_user,:on=>:froms %></td>
        <td><%= render 'tags/input',:taggable=>user,:owner=>current_user,:on=>:tos %></td>
        <td><%= render 'tags/input',:taggable=>user,:owner=>current_user,:on=>:cars %></td>
      </tr>
    <% end %>
  </tbody>
</table>